{
  "extends": "airbnb-base",
  "env": {
      "node": false,
      "jquery": true,
      "browser": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "rules": {
    "no-template-curly-in-string": "warn",
    "prefer-rest-params": "off",
    "prefer-template": "off",
    "object-shorthand": "off"
  }
}
